#image.p-20
  #unlogin(ng-if="!login")
    span.ct-red 请先登录
  #islogin(ng-if="login")
    #file-list
      div(layout="row" layout-wrap)
        div(ng-repeat="file in user.files" flex="20" ng-if="file.kind == 1").m-20
          div(layout="row")
            img.preview-img(ng-src="{{file.url}}" ng-click="vm.preview(file, $event)")
            md-button(ng-click="vm.delete(file)") 删除